	automatic [label="automatic-install"];
	pkgdict -> short_desc [label="string"]
	pkgdict -> requiredby [label="array"];
	requiredby [style=filled];
	requiredby -> reqby1 [label="string"];
	requiredby -> reqby2 [style=filled,label="string"];
